{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "react-app"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "react/prop-types": "off",
        "no-control-regex": "warn"
    },
    "ignorePatterns": ["**/tests/*.js", "**/*.test.js", "**/build/*"]
}
